#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/konra/node_modules/.pnpm/ts-jest@29.4.1_@babel+core@_4ce987c2017acf4e0c14172db3b5cbee/node_modules/ts-jest/node_modules:/mnt/c/Users/konra/node_modules/.pnpm/ts-jest@29.4.1_@babel+core@_4ce987c2017acf4e0c14172db3b5cbee/node_modules:/mnt/c/Users/konra/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/konra/node_modules/.pnpm/ts-jest@29.4.1_@babel+core@_4ce987c2017acf4e0c14172db3b5cbee/node_modules/ts-jest/node_modules:/mnt/c/Users/konra/node_modules/.pnpm/ts-jest@29.4.1_@babel+core@_4ce987c2017acf4e0c14172db3b5cbee/node_modules:/mnt/c/Users/konra/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../node_modules/.pnpm/ts-jest@29.4.1_@babel+core@_4ce987c2017acf4e0c14172db3b5cbee/node_modules/ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../../../../../node_modules/.pnpm/ts-jest@29.4.1_@babel+core@_4ce987c2017acf4e0c14172db3b5cbee/node_modules/ts-jest/cli.js" "$@"
fi
